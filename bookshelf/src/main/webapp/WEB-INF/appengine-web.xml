<!-- [START example] -->
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <sessions-enabled>true</sessions-enabled>
  <threadsafe>true</threadsafe>
  <vm>true</vm>
  <!-- TODO remove beta-settings -->
  <beta-settings>
    <setting name="java_quickstart" value="true" />
  </beta-settings>
  <env-variables>
    <env-var name="STORAGETYPE" value="YOUR-DATABASE-CHOICE" />
    <env-var name="SQL_DATABASE_URL" value="jdbc:mysql://HOST-IP:3306/YOUR-DB-NAME?user=USERNAME&amp;password=PASSWORD" />
    <env-var name="PROJECT_ID" value="YOUR-PROJECT-ID" />
    <env-var name="BUCKET_NAME" value="YOUR-BUCKET-NAME" />
    <env-var name="CLIENT_ID" value="YOUR-CLIENT-ID" />
    <env-var name="CLIENT_SECRET" value="YOUR-CLIENT-SECRET" />
    <!-- callback url should be the one registered in Google Developers Console -->
    <env-var name="CALLBACK_URL" value="YOUR-CALLBACK-URL" />
  </env-variables>
  <!-- local logging is fixed by PR 136 and 148 appengine java vm runtime on github -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
  </system-properties>
</appengine-web-app>
<!-- [END example] -->
